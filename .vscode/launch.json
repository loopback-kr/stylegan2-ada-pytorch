{
    // IntelliSense를 사용하여 가능한 특성에 대해 알아보세요.
    // 기존 특성에 대한 설명을 보려면 가리킵니다.
    // 자세한 내용을 보려면 https://go.microsoft.com/fwlink/?linkid=830387을(를) 방문하세요.
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: 현재 파일",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "justMyCode": true
        },
        {
            "name": "Python: train.py",
            "type": "python",
            "request": "launch",
            "program": "train.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}",
            "justMyCode": true,
            "args": [
                "--outdir",
                "outputs/benign_nomask",
                "--data",
                "data/dataset_gan_resize_256/benign_nomask.zip",
                "--gpus",
                "4",
                "--kimg",
                "25000",
                "--metrics",
                "fid50k_full",
                "--cfg",
                "auto",
                "--aug",
                "ada",
                "--cond",
                "0",
                "--augpipe",
                "bgc_norotate"
                // "--snap",
                // "10"
                // "--dry-run"
            ],
            "env": {
                "CUDA_VISIBLE_DEVICES": "0,1,2,3"
            }
        }
    ]
}